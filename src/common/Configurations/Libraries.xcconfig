THREE20CORE322_LIB         = -force_load "$(CONFIGURATION_BUILD_DIR)/libThree20Core-Xcode3.2.2.a"
THREE20NETWORK322_LIB      = -force_load "$(CONFIGURATION_BUILD_DIR)/libThree20Network-Xcode3.2.2.a"
THREE20STYLE322_LIB        = -force_load "$(CONFIGURATION_BUILD_DIR)/libThree20Style-Xcode3.2.2.a"
THREE20UICOMMON322_LIB     = -force_load "$(CONFIGURATION_BUILD_DIR)/libThree20UICommon-Xcode3.2.2.a"
THREE20UINAVIGATOR322_LIB  = -force_load "$(CONFIGURATION_BUILD_DIR)/libThree20UINavigator-Xcode3.2.2.a"
